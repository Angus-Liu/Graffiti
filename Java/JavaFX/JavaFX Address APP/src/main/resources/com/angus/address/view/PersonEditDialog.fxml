<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="280.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.angus.address.view.PersonEditDialogController">
    <GridPane layoutX="39.0" layoutY="60.0" minWidth="-Infinity" prefHeight="199.0" prefWidth="284.0"
              AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="120.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="240.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label text="First Name"/>
        <TextField fx:id="firstNameField" GridPane.columnIndex="1"/>
        <Label text="Last Name" GridPane.rowIndex="1"/>
        <TextField fx:id="lastNameField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Label text="Street" GridPane.rowIndex="2"/>
        <TextField fx:id="streetField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Label text="City" GridPane.rowIndex="3"/>
        <TextField fx:id="cityField" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <Label text="Postal Code" GridPane.rowIndex="4"/>
        <TextField fx:id="postalCodeField" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <Label text="Birthday" GridPane.rowIndex="5"/>
        <TextField fx:id="birthdayField" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
    </GridPane>
    <HBox layoutX="151.0" layoutY="215.0" spacing="5.0" AnchorPane.bottomAnchor="10.0"
          AnchorPane.rightAnchor="10.0">
        <Button onAction="#handleOK" mnemonicParsing="false" prefWidth="90.0" text="OK"/>
        <Button mnemonicParsing="false" prefWidth="90.0" text="Cancel"/>
    </HBox>
</AnchorPane>
